{
  "name": "optimoapps/jolo-api",
  "description": "A Laravel package for Jolo API money transfer",
  "keywords": [
    "jolo api",
    "laravel package sms",
    "laravel package money transfer",
    "sms",
    "jolo money transfer"
  ],
  "type": "library",
  "require": {
    "php": ">=7.1",
    "illuminate/support": "^6.0|^7.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4|^9.0.*",
    "orchestra/testbench": "3.8.*|4.* ",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "OptimoApps\\JoloApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OptimoApps\\JoloApi\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimoApps\\JoloApi\\JoloApiServiceProvider"
      ],
      "aliases": {
        "JoloApi": "OptimoApps\\JoloApi\\JoloApiFacade"
      }
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "satz",
      "email": "sathish.thi@gmail.com",
      "homepage": "https://www.optimoapps.com/",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable"
}
